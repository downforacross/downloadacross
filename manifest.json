{
  "name": "Download a Cross",
  "description": "Browse crossword puzzles from the archives of all major crossword publishers",
  "version": "0.4.0",
  "permissions": [
    "storage",
    "downloads",
    "unlimitedStorage"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "Download a Cross",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
